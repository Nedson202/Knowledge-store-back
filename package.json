  "main": "index.js",
  "scripts": {
    "start": "babel-node index",
    "start:dev": "nodemon --exec babel-node index",
    "migrate": "babel-node database/createMigration.js",
    "undo:migrate": "babel-node database/dropMigration.js",
    "add:seed": "babel-node database/createSeeds.js",
    "undo:seed": "babel-node database/dropSeeds.js",
    "seed": "npm run undo:seed && npm run add:seed",
    "trigger:migration": "npm run migrate && npm run seed",
    "test": "cross-env NODE_ENV=test jest --collectCoverage --runInBand --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --collectCoverage --runInBand --detectOpenHandles --watchAll",
    "pretest": "cross-env NODE_ENV=test npm run undo:migrate && cross-env NODE_ENV=test npm run trigger:migration",
    "heroku-postbuild": "npm run trigger:migration"
  },
